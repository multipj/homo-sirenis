  },
  "dependencies": {
    "@websitebeaver/vue-magnifier": "^1.0.0",
    "core-js": "^3.8.3",
    "jquery": "^3.6.1",
    "marked": "^4.1.1",
    "vue": "^3.2.13",
    "vue-image-magnifier": "^0.2.1",
    "vue-router": "^4.0.13",
    "vue3-markdown-it": "^1.0.10"
  },
  "devDependencies": {
    "@babel/core": "^7.12.16",
    "@babel/eslint-parser": "^7.12.16",
    "@vue/cli-plugin-babel": "~5.0.0",
    "@vue/cli-service": "^5.0.1",
    "node-sass": "^7.0.3",
    "raw-loader": "^4.0.2",
    "sass-loader": "^12.6.0",
    "style-loader": "^3.3.1",
    "style-resources-loader": "^1.5.0",
    "vue-loader": "^17.0.0",
    "vue-template-compiler": "^2.6.14"
  },
  "eslintConfig": {
