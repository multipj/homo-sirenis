{
  "name": "homo-sirenis",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build --mode production",
    "lint": "vue-cli-service lint",
    "deploy-gh": "npm run build && git add -f dist && git commit -m 'new deployment' && git push -f git@github.com:dkaerit/homo-sirenis.git master:gh-pages",
